{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "12.15.0",
      "name": "debug",
      "skipFiles": [
          "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/@keystonejs/keystone/bin/cli.js",
    },
    {
      "name": "npm build",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeVersion": "12.15.0",
      "runtimeArgs": ["run", "build"]
    },
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "12.15.0",
      "name": "build",
      "skipFiles": [
          "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/@keystonejs/keystone/bin/cli.js",
      "args": [
        "build"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "12.15.0",
      "name": "start",
      "skipFiles": [
          "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/@keystonejs/keystone/bin/cli.js",
      "args": [
        "start"
      ]
    }
  ]
}
